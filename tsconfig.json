        "./dist/identity"
      ],
      "infrastructure": [
        "./dist/infrastructure"
      ],
      "iot": [
        "./dist/iot"
      ],
      "maintenance": [
        "./dist/maintenance"
      ],
      "monitoring": [
        "./dist/monitoring"
      ],
      "ngrx-store": [
        "./dist/ngrx-store"
      ],
      "reports": [
        "./dist/reports"
      ],
      "routes": [
        "./dist/routes"
      ],
      "sanitation-layout": [
        "./dist/sanitation-layout"
      ],
      "session": [
        "./dist/session"
      ],
      "settings": [
        "./dist/settings"
      ],
      "template-smart-sanitation": [
        "./dist/template-smart-sanitation"
      ],
      "tes-page": [
        "./dist/tes-page"
      ],
      "test-store-lib": [
        "./dist/test-store-lib"
      ],
      "xmaint-asset-lib": [
        "./dist/xmaint-asset-lib"
      ],
      "xmaint-core-lib": [
        "./dist/xmaint-core-lib"
      ],
      "xmaint-dashboard": [
        "./dist/xmaint-dashboard"
      ],
      "xmaint-dashboard-lib": [
        "./dist/xmaint-dashboard-lib"
